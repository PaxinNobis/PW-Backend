{
  "name": "astrotv-backend",
  "version": "1.0.0",
  "description": "Backend API para la plataforma de streaming AstroTV",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npx prisma generate && tsc",
